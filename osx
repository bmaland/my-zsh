# (-*- shell-script -*-)

unalias ls

alias o="open"
alias emacs="open -a /Applications/Aquamacs\ Emacs.app/"

# open ftw
alias -s {doc,odt,rtf,jpeg,jpg,JPEG,JPG,png,gif,xpm,avi,mkv,mpg,mpeg,vmw,mov,rm,pdf}=open

export BROWSER="open"

# Avoid "unsupported locale"-errors on osx - setting lang vars back to default
export LC_ALL="en_US.UTF-8"
export LANG="en_US.UTF-8"
